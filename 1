///a
abstract class Caaa {
    protected int num;

    public Caaa(int n) {
        num = n;
    }

    public abstract void show();
}
///b、c
class Cbbb extends Caaa {
    public Cbbb(int n) {
        super(n); // 呼叫父類別的建構子
    }

    public void show() {
        System.out.println("num-" + num);
    }
}
