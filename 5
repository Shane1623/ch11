interface iAaa {
    void show();
}

class Bbb {
    public int num = 10;

    public void set(int n) {
        num = n;
    }
}

class Ccc extends Bbb implements iAaa {
    // 實作 iAaa 介面的 show 函數
    public void show() {
        System.out.println("num-" + num);
    }
}

public class EX11_5 {
    public static void main(String[] args) {
        Ccc obj = new Ccc();
        obj.set(5);
        obj.show(); // 印出 num=5
    }
}
